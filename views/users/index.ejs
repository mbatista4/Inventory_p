<h2 class="page-header"><a href="/users/">Search Users</a></h2>

<form action="/users/delete?_method=DELETE" method="POST">
    <button type="submit">delete</button>
</form>
<h5><a href="/users/logout" class="">Logout</a></h5>
<form action="/users" method="get">
    <div class="form-row">
        <div class="form-item">
            <label for="User">Name</label>
            <input type="text" name="name" value="<%= searchOptions.name || '' %>">
        </div>
    </div>
    <div class="form-row form-row-end">
        <button class="btn btn-primary" type="submit">Search</button>
    </div>
</form>
<br>
<% users.forEach(user => { %>
<div class="user-row">
    <div class="user-name"><%= user.name %></div>
    <div class="btn-row">
        <a class="btn btn-primary" href="/users/<%= user.id %>">View</a>
        <%-// include('../partials/deleteForm', {url : `/users/${user.id}` }) %>
    </div>
</div>
<% }) %>